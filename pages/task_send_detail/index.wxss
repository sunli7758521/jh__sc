.table {
  margin-top:20rpx;
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
  font-size: 9px;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
    font-size: 8px;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 40%;
  justify-content: center;
  background: #45d989;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}